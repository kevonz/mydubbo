				<version>2.1-alpha-1</version>
				<configuration>
					<webResources>
						<!-- 对需要修改参数的配置文件进行配置 filtering -->
						<resource>
							<directory>src/main/resources/config/properties/${mavenDir}</directory>
							<targetPath>WEB-INF/classes/config/properties</targetPath>
							<!--<filtering>true</filtering>-->
						</resource>
					</webResources>
				</configuration>
			</plugin>
